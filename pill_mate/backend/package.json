{
    "name": "backend",
    "version": "0.0.0",
    "scripts": {
        "build": "tsc",
        "start": "node build/index.js",
        "lint": "eslint .",
        "test": "jest",
        "test-watch": "jest --watch",
        "swagger-generate": "swagger-jsdoc -d swagger.cjs src/**/*.ts",
