        "@types/validator": "^13.12.2",
        "@types/ws": "^8.5.14",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "morgan": "^1.10.0",
        "reflect-metadata": "^0.2.2",
        "sequelize": "^6.37.5",
        "sequelize-typescript": "^2.1.6",
        "sqlite3": "^5.1.7",
        "typescript": "^5.7.3",
        "winston": "^3.17.0",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.16.0",
        "@types/jest": "^29.5.14",
